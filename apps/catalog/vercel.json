{
  "buildCommand": "cd ../.. && npx nx build catalog --prod",
  "outputDirectory": "../../dist/apps/catalog/.next",
  "framework": "nextjs",
  "installCommand": "cd ../.. && npm install",
  "devCommand": "cd ../.. && npx nx serve catalog",
  "env": {
    "DATABASE_URL": {
      "type": "secret"
    }
  },
  "regions": [
    "iad1"
  ],
  "functions": {
    "api/**/*.ts": {
      "maxDuration": 10
    }
  }
}